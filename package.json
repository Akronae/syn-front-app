{
  "name": "syneidesis",
  "version": "1.0.0",
  "description": "A nice cool-kid project",
  "author": "Alexandre Daubricourt",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --hot --inline --progress --config build/webpack.dev.conf.js",
    "build": "cross-env node build/build.js",
    "build:dev": "cross-env NODE_ENV='development' BUILD_TYPE='production' npm run build",
    "build:prod": "cross-env NODE_ENV='production' BUILD_TYPE='production' npm run build",
    "build:watch": "cross-env WEBPACK_WATCH=true npm run build",
    "launch:android": "npx cap run android",
    "launch:ios": "cordova run ios",
    "deploy-dev:android": "cross-env npm run build:dev && npm run launch:android",
    "deploy-prod:android": "cross-env npm run build:prod && npm run launch:android",
    "deploy-dev:ios": "cross-env npm run build:dev && npm run launch:ios",
    "deploy-prod:ios": "cross-env npm run build:prod && npm run launch:ios"
  },
  "dependencies": {
