{
  "name": "ex",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web --clear",
    "pretty": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,yml,yaml,md}\" \"./*.{js,jsx,ts,tsx,json,yml,yaml,md}\"",
    "lint": "eslint App.tsx src --fix",
    "format": "yarn pretty && yarn lint",
    "build:android:apk": "rm *.apk; eas build -p android --profile preview --local",
    "log:android": "npx react-native log-android",
    "log:ios": "npx react-native log-ios"
  },
  "dependencies": {
